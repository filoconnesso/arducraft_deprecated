{
  "name": "arducraft-gui",
  "version": "1.0.1",
  "description": "Arducraft",
  "main": "arducraft-gui.js",
  "scripts": {
    "start-gui": "electron arducraft-gui",
    "package-win-x64": "electron-packager ./ ArducraftConnect --platform=win32 --arch=x64 --icon=assets/images/icon.png --asar --out=build --overwrite",
    "package-win-x86": "electron-packager ./ ArducraftConnect --platform=win32 --arch=ia32 --icon=assets/images/icon.png --asar --out=build --overwrite",
    "package-linux-x64": "electron-packager ./ ArducraftConnect --platform=linux --arch=x64 --icon=assets/images/icon.png --asar --out=build --overwrite",
    "package-linux-x86": "electron-packager ./ ArducraftConnect --platform=linux --arch=ia32 --icon=assets/images/icon.png --asar --out=build --overwrite",
    "package-macOS-x64": "electron-packager ./ ArducraftConnect --platform=darwin --arch=x64 --icon=assets/images/icon.png --asar --out=build --overwrite",
    "package-all": "npm run package-win-x64 & npm run package-win-x86 & npm run package-linux-x64 & npm run package-linux-x86 & npm run package-macOS-x64"
  },
  "repository": {
    "type": "git",
    "url": "\t"
  },
  "author": "Filo Connesso",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "electro": "^2.1.1",
    "electron": "^21.2.2",
    "electron-packager": "^15.4.0"
  },
  "dependencies": {
    "minecraft-protocol": "^1.34.0",
    "mineflayer": "^4.5.0",
    "rxjs": "^7.5.5",
    "serialport": "^10.4.0"
  }
}
