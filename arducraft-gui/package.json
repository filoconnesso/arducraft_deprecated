{
  "name": "arducraft-gui",
  "version": "1.0.0",
  "description": "Arducraft",
  "main": "arducraft-gui.js",
  "scripts": {
    "start-gui": "electron arducraft-gui",
    "package-win-x64" : "electron-packager ./ ArduCraft-Gui --platform=win32 --arch=x64 --asar --out=build --overwrite",
    "package-win-x86" : "electron-packager ./ ArduCraft-Gui --platform=win32 --arch=ia32 --asar --out=build --overwrite",
    "package-linux-x64" : "electron-packager ./ ArduCraft-Gui --platform=linux --arch=x64 --asar --out=build --overwrite",
    "package-linux-x86" : "electron-packager ./ ArduCraft-Gui --platform=linux --arch=ia32 --asar --out=build --overwrite",
    "package-macOS-x64" : "electron-packager ./ ArduCraft-Gui --platform=darwin --arch=x64 --asar --out=build --overwrite",
    "package-all" : "npm run package-win-x64 & npm run package-win-x86 & npm run package-linux-x64 & npm run package-linux-x86 & npm run package-macOS-x64"
  },
  "repository": {
    "type": "git",
    "url": "\t"
  },
  "author": "Filo Connesso",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "electro": "^2.1.1",
    "electron": "^18.0.4",
    "electron-packager": "^15.5.1"
  },
  "dependencies": {
    "mineflayer": "^4.9.0",
    "rxjs": "^7.8.1",
    "serialport": "^10.5.0"
  }
}
